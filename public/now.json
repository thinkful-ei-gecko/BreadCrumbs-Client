{
  "version": 1,
  "name": "Bread-Crumbs-App",
  "alias": "Bread-Crumbs-App",
  "routes": [
      {
        "src": "^/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "^/favicon.ico$",
        "dest": "/favicon.ico"
      },
      {
        "src": "^/manifest.json$",
        "dest": "/manifest.json"
      },
      {
        "src": ".*",
        "dest": "/index.html"
      }
  ]
}